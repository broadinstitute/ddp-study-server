<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="DDP-7641_add_kit_participant_notifications_activated_to_osteo2" author="nghurtch">
        <insert tableName="ddp_instance_role">
            <column name="ddp_instance_id" valueComputed="(SELECT ddp_instance_id from ddp_instance WHERE instance_name = 'osteo2')"/>
            <column name="instance_role_id" valueComputed="(SELECT instance_role_id FROM instance_role WHERE name = 'kit_participant_notifications_activated')"/>
        </insert>
    </changeSet>
</databaseChangeLog>
