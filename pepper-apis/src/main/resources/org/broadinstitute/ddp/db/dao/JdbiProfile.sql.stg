group JdbiProfile;

createUserProfileStmt() ::= <<
    INSERT INTO user_profile
        (
            user_id,
            first_name,
            last_name,
            sex,
            birth_date,
            preferred_language_id,
            do_not_contact
        )
    VALUES
        (
            :userId,
            :firstName,
            :lastName,
            :sex,
            :birthDate,
            :preferredLanguageId,
            :doNotContact
        )
>>
