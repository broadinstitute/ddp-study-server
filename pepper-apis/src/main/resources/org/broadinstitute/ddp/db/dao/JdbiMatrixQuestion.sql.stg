group JdbiMatrixQuestion;

insertMatrixByModeIds() ::= <<
insert into matrix_question
       (question_id, matrix_select_mode_id)
values (:questionId, :selectModeId)
>>

insertMatrixByModeCodes() ::= <<
insert into matrix_question
       (question_id, matrix_select_mode_id)
values (:questionId,
       (select matrix_select_mode_id from matrix_select_mode where matrix_select_mode_code = :selectMode))
>>
