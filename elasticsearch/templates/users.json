{
  "index_patterns": ["users.*"],
  "settings": {
    "index": {
      "number_of_shards": "1",
      "auto_expand_replicas": "0-1"
    }
  },
  "mappings": {
    "_doc": {
      "properties": {
        "profile": {
          "properties": {
            "guid": {
              "type": "keyword"
            },
            "hruid": {
              "type": "keyword"
            },
            "legacyAltPid": {
              "type": "keyword"
            },
            "legacyShortId": {
              "type": "keyword"
            },
            "firstName": {
              "type": "text"
            },
            "lastName": {
              "type": "text"
            },
            "email": {
              "type": "keyword",
              "fields": {
                "text": {
                  "type": "text"
                }
              }
            },
            "doNotContact": {
              "type": "boolean"
            },
            "createdAt": {
              "type": "date",
              "ignore_malformed": false,
              "format": "strict_date_time||strict_date_time_no_millis||epoch_millis"
            }
          }
        },
        "proxies": {
          "type": "keyword"
        },
        "governedUsers": {
          "type": "keyword"
        }
      }
    }
  }
}
